<UserControl x:Class="DATApp.MVVM.View.Controls.LoginButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DATApp.MVVM.View.Controls"
             xmlns:viewmodel="clr-namespace:DATApp.MVVM.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <UserControl.DataContext>
        <viewmodel:MainWindowViewModel />
    </UserControl.DataContext>
    
    <Grid>
        <Button Content = "Log ind" Command="{Binding MenuViewCommand}" Height="20"  Width="150" VerticalAlignment="Bottom" Margin="0,0,0,35"/>
    </Grid>
</UserControl>
